<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Display</title>
    </head>
    <link rel="stylesheet" href="/stylesheets/display.css">
    <body>
        <div class="box-1">
            <h2>Displays Page</h2>
            <% rows.forEach(function(r) {%>
                <div class="box-2">
                    <p><%=r.name%></p>
                    <canvas id="text" width="<%=r.weight%>" height="<%=r.height%>" style="border:1px solid black"></canvas>
                </div>
             <%});%>
            <br>
            <table class="box-3">
                    <tr><form action="/home" method="POST">
                        <button type="submit">Home</button>
                    </form></tr>
                    <tr><form action="/tokimon" method="POST">
                        <button type="submit">Tokimon</button>
                    </form></tr>
            </table>
        </div>
    </body>
</html>